User json
    firstName Text
    lastName Text
    grade Text
    chipId Text -- TODO: Unique
    username Text -- TODO: Unique
    password Text Maybe
    fails Int
    access [Access]
    roles Role
    UniqueUser username -- Compatibility for Yesod.Auth.HashDB
    deriving Show Eq
Access json
    time UTCTime
    inside Bool
    deriving Eq Show
Role json
    citizen Bool
    representative Bool
    admin Bool
    teacher Text Maybe
    deriving Show Eq
Choice json
    identity Int -- TODO: Unique
    votes Int
    description Text
    deriving Show Eq
Vote json
    identity Int -- TODO: Unique
    description Text
    voted [UserId]
    choices [Choice]
    deriving Show Eq
