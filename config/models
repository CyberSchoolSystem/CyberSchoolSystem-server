User json
    firstName Text
    lastName Text
    grade Text
    chipId Text -- TODO: Unique
    username Text -- TODO: Unique
    password Text Maybe
    fails Int
    access [Access]
    UniqueUser username -- Compatibility for Yesod.Auth.HashDB
Access json
    time UTCTime
    inside Bool
    deriving (Eq)
Choice json
    identity Int -- TODO: Unique
    votes Int
    description Text
    deriving (Show)
Vote json
    identity Int -- TODO: Unique
    description Text
    voted [UserId]
    choices [Choice]
    deriving (Show) -- TODO: Debug. Remove
