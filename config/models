User json
    firstName Text
    lastName Text
    grade GradeId
    username Text -- TODO: Unique
    password Text Maybe
    fails Int
    access [Access]
    roles Role
    UniqueUser username -- Compatibility for Yesod.Auth.HashDB
    deriving Show Eq
Access json
    time UTCTime
    inside Bool
    deriving Eq Show
Grade json
    name Text
Role json
    citizen Bool
    representative Bool
    admin Bool
    teacher GradeId Maybe
    customs Bool
    tech Bool
    deriving Show Eq
Choice json
    identity Int -- TODO: Unique
    votes Int
    description Text
    deriving Show Eq
Vote json
    title Text
    description Text
    voted [UserId]
    choices [Choice]
    endOfLife UTCTime
    deriving Show Eq
File
    filePath FilePath
    mime ContentType
    title Text
    owner UserId
    destroy UTCTime
